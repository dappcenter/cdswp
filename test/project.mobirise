          "_name": "social-buttons2",
          "_customHTML": "<section group=\"Social\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" plugins=\"SocialLikes\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input inline type=\"range\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input inline type=\"range\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n        \n        <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#ff3366\"> \n        <select title=\"Icons\" name=\"iconsCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n        </select>\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background4.jpg\" parallax>\n            <input type=\"color\" title=\"Background Color\" value=\"#ffc4e3\" selected>\n            <input type=\"video\" title=\"Background Video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        <input type=\"color\" title=\"color2\" name=\"color2\" condition=\"bg.type=='color'\" value=\"#ffffff\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"media-container-row\">\n            <div class=\"col-md-8 align-center\">\n                <h2 class=\"pb-3 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"></h2>\n            \n                <div class=\"social-list pl-0 mb-0\">\n                    <a href=\"https://t.me/candyswap\" target=\"_blank\">\n                        <span mbr-icon class=\"px-2 mbr-iconfont mbr-iconfont-social socicon-telegram socicon\"></span>\n                    </a>\n                    <a href=\"https://twitter.com/candyswaplink\" target=\"_blank\">\n                        <span mbr-icon class=\"px-2 mbr-iconfont mbr-iconfont-social socicon-twitter socicon\" mbr-if=\"iconsCount > 1\"></span>\n                    </a>\n                    \n                        <span mbr-icon class=\"px-2 mbr-iconfont mbr-iconfont-social socicon-medium socicon\" mbr-if=\"iconsCount > 2\"></span>\n                    \n                    <a href=\"https://www.youtube.com/c/mobirise\" target=\"_blank\">\n                        <span mbr-icon class=\"px-2 socicon-youtube socicon mbr-iconfont mbr-iconfont-social\" mbr-if=\"iconsCount > 3\"></span>\n                    </a>\n                    <a href=\"https://www.behance.net/Mobirise\" target=\"_blank\">\n                        <span mbr-icon class=\"px-2 socicon-behance socicon mbr-iconfont mbr-iconfont-social\" mbr-if=\"iconsCount > 4\"></span>\n                    </a>\n                </div>\n            </div>\n        </div>\n    </div>\n   <script>\nvar started=0;\t\n\n\nfunction getUrlVars() {\n\t\t\tvar vars = {};\n\t\t\tvar parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function (m, key, value) {\n\t\t\t\tvars[key] = value;\n\t\t\t});\n\treturn vars;\n}\n\t\n\nfunction roundToFour(num) {\n\treturn +(Math.floor(num + \"e+4\") + \"e-4\");\n}\nfunction roundToTwo(num) {\n\treturn +(Math.floor(num + \"e+2\") + \"e-2\");\n}\nfunction round(num) {\n\treturn +(Math.floor(num + \"e+0\") + \"e-0\");\n}\n  \n\nvar contractAddress='TXeg4F1o8QcNcEWwoqBSb5ayk8FfKdrLbH';\n\n\n\n  var toinvest=0;\n  var myaddress;\n\n\n\n\n    \nasync function start() {\nstarted=1;\n  \nlet MyContract = await tronWeb.contract().at(contractAddress);\n \n\n  \n   \n  let _price= await MyContract.price().call(); \n    document.getElementById(\"price\").innerHTML=roundToFour(_price/1000000);\n\n  \n  \n  let _candybalance= await MyContract.candybalance().call(); \n    document.getElementById(\"available\").innerHTML=roundToTwo(_candybalance/1000000);\n\n\n \n}\n    \n   async function invest() {\n\tif(started>0){\t\n\n    var tosend = parseFloat(document.getElementsByName('ETH')[0].value*1000000);\n\nif(tosend>=100000000){\n\t\n  let contractInstance = await tronWeb.contract().at(contractAddress);\n\t\n\tlet args = {\n\t\tcallValue: window.tronWeb.toHex(round(tosend)),\n      \tfeeLimit: 10000000,\n\t\tshouldPollResponse: true\n\t};\n      \nlet result = await contractInstance.buy().send(args);\n      start();\n} else alert('Minimum Investment is 100 TRX');\n} else alert ('Please unlock your Tron Wallet');\n}\n      \n    \n \n      \n    \n   \n    \n    \n    \n    \naccountInterval2 = setInterval(function () {\n  if(myaddress==false&&tronWeb.defaultAddress.base58 != false) location.reload();\n    \n    if(myaddress!=tronWeb.defaultAddress.base58){\n        myaddress = tronWeb.defaultAddress.base58;\n\t\t\t\n}\n  \n\tif (typeof window.tronWeb !== 'undefined'){\t\t\n\n  if(tronWeb.defaultAddress.base58 != false){\ndocument.getElementById(\"installmetamask\").style.display = \"none\";\n  console.log('started');\n   start();\n}}\n}, 2000);   \n  </script>\n</section>",
